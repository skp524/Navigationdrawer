<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_dashboard">



    <fragment
        android:id="@+id/nav_dashboard"
        android:name="com.shashank.navigationdrawer.Dashboard.DashboardFragment"
        android:label="@string/dashboard"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.shashank.navigationdrawer.Dashboard.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/nav_my_booking"
        android:name="com.shashank.navigationdrawer.Dashboard.MyBookingFragment"
        android:label="@string/my_booking"
        tools:layout="@layout/fragment_my_booking" />

    <fragment
        android:id="@+id/nav_logout"
        android:name="com.shashank.navigationdrawer.Dashboard.LogoutFragment"
        android:label="@string/logout"
        tools:layout="@layout/fragment_logout" />

</navigation>